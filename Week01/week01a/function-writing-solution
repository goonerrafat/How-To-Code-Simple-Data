#reader(lib "htdp-beginner-reader.ss" "lang")((modname function-writing-solution) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
;defining the function

(define (largest a b)
  (if (> a b)
      a
      b))

;testing it
(largest 6 4)
(largest 4 5)